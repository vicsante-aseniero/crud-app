    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080"
        },
        {
            "name": "Http API Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/app/main.go"
        },
        {
            "name": "gRPC API Server - Company Domain",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/company/main.go"
        },
        {
            "name": "App HTTP Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/app/http/server.go"
        },
        {
            "name": "App gRPC Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/app/rpc/server.go"
        }
    ]
}